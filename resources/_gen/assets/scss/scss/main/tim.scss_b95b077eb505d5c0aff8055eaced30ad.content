.autocomplete{position:relative;display:inline-block}input{border:1px solid transparent;background-color:#f1f1f1;padding:10px;font-size:16px}input[type=text]{background-color:#f1f1f1;width:100%}input[type=submit]{background-color:DodgerBlue;color:#fff}.autocomplete-items{position:absolute;border:1px solid #d4d4d4;border-bottom:none;border-top:none;z-index:99;top:100%;left:0;right:0}.autocomplete-items div{padding:10px;cursor:pointer;background-color:#fff;border-bottom:1px solid #d4d4d4}.autocomplete-items div:hover{background-color:#e9e9e9}.autocomplete-active{background-color:DodgerBlue!important;color:#fff}#tim .container{margin-left:2vw;margin-right:2vw;max-width:96vw}#tim .nowrap{flex:nowrap;align-items:stretch}#tim .navbar{width:20%;display:block}#tim .navbar .input-icon i{position:absolute;padding:8px}#tim .navbar input{padding:5px 0 5px 40px;margin-bottom:20px;box-shadow:0 4px 8px 0 rgba(0,0,0,.6)}#tim #listing{flex-grow:1;transition:margin-left .5s;padding:16px;border-right:solid 1px gray;box-shadow:0 8px 8px 0 rgba(0,0,0,.6)}#tim #sidebar .row{justify-content:space-around;font-size:.8rem;text-align:center;padding:0 10px 10px}#tim #sidebar [class^=icon]{color:darkred;font-size:1.5rem;margin:0}#tim #sidebar hr{margin-bottom:20px}#tim #sidebar label{font-size:.8rem}#tim #sidebar .label{width:50%;text-align:left;padding-left:20px;font-weight:600}#tim #sidebar select,#tim #sidebar input{width:90%;border:0;background:#fff;font-size:.8rem;padding:0}#tim #sidebar .select,#tim #sidebar .input{flex-grow:1;flex:1}#tim #sidebar #edit:hover,#tim #sidebar #preview:hover,#tim #sidebar #copy:hover,#tim #sidebar #archive:hover{opacity:.7}#tim #sidebar #save button{width:initial;background-color:darkred;color:#fff;padding:5px 20px;margin:20px 0;box-shadow:0 4px 6px 0 rgba(0,0,0,.6)}#tim .sidebar{height:100%;width:0;z-index:1;overflow-x:hidden;transition:.5s}#tim .sidebar a{padding:8px 8px 8px 32px;text-decoration:none;font-size:25px;color:#818181;display:block;transition:.3s}#tim .sidebar a:hover{color:#f1f1f1}#tim .sidebar .closebtn{position:absolute;top:0;right:25px;font-size:36px;margin-left:50px}#tim *{box-sizing:border-box}#tim .autocomplete{width:300px}#tim a.link-unstyled{font-weight:400;color:#000}#tim .table{display:flex;flex-direction:row;justify-content:space-between;width:100%;flex-wrap:wrap;padding-right:15px;padding-left:15px;margin-right:auto;margin-left:auto}#tim .item-1,#tim .item-2{width:30%}#tim .item-date{width:13%}#tim .item-icon{width:3%}#tim #url{color:gray;text-align:left;padding-left:20px}#tim .noshow:focus{background-color:#f0f8ff}#tim ul{list-style:none;margin-left:-40px;margin-bottom:30px}#tim li{margin-bottom:10px}#tim button{background-color:#add8e6;margin-bottom:20px;width:100%;height:38px;box-shadow:0 4px 8px 0 rgba(0,0,0,.6)}#tim [class^=icon]{line-height:1em;width:1em;margin-right:15px;display:inline-block}#tim .icon:hover:after{display:block;position:relative}#tim span.mouse{line-height:1.2rem}#tim span.mouse:hover{cursor:pointer;font-weight:600}#tim span.mouse:hover i{color:darkred;transform:scale(1.5);transform-origin:50% 50%;line-height:1em;-webkit-font-smoothing:antialiased;width:1em;display:inline-block}#tim .noshow:focus{background-color:orange;border:solid 1px}#tim .odd{background-color:#b8d1f3}#tim .even{background-color:#dae5f4}