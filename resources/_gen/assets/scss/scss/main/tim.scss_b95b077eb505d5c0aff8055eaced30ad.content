#tim h2{background-color:coral;padding:5px;margin-top:0}#tim *{box-sizing:border-box}#tim .autocomplete{position:relative;display:inline-block}#tim input{border:1px solid transparent;background-color:#f1f1f1;padding:5px;font-size:.9rem}#tim input[type=text]{background-color:#f1f1f1;width:100%}#tim input[type=submit]{background-color:DodgerBlue;color:#fff}#tim .autocomplete-items{position:absolute;border:1px solid #d4d4d4;border-bottom:none;border-top:none;z-index:99;top:100%;left:0;right:0}#tim .autocomplete-items div{padding:5px;cursor:pointer;background-color:#fff;border-bottom:1px solid #d4d4d4}#tim .autocomplete-items div:hover{background-color:#e9e9e9}#tim .autocomplete-active{background-color:DodgerBlue!important;color:#fff}