.autocomplete{position:relative;display:inline-block}input{border:1px solid transparent;background-color:#f1f1f1;padding:10px;font-size:16px}input[type=text]{background-color:#f1f1f1;width:100%}input[type=submit]{background-color:DodgerBlue;color:#fff}.autocomplete-items{position:absolute;border:1px solid #d4d4d4;border-bottom:none;border-top:none;z-index:99;top:100%;left:0;right:0}.autocomplete-items div{padding:10px;cursor:pointer;background-color:#fff;border-bottom:1px solid #d4d4d4}.autocomplete-items div:hover{background-color:#e9e9e9}.autocomplete-active{background-color:DodgerBlue!important;color:#fff}#tim .nowrap{flex-wrap:nowrap;align-items:stretch;margin-bottom:20px}#tim *{box-sizing:border-box}#tim .autocomplete{width:300px}#tim a.link-unstyled{font-weight:400;color:#000}#tim [class^=icon]{line-height:1em;width:1em;margin-right:15px;display:inline-block}#tim .icon:hover:after{display:block;position:relative}#tim #navbar{width:230px;display:block;background-color:#d3d3d31c;transition:.5s}#tim #navbar .wrapper{padding:20px}#tim #navbar ul{list-style:none;margin-left:-40px;margin-bottom:30px}#tim #navbar li{margin-bottom:10px}#tim #navbar button{background-color:#add8e6;margin-bottom:20px;width:100%;height:39px;box-shadow:0 4px 8px 0 rgba(0,0,0,.6)}#tim #navbar span.mouse{line-height:1.2rem}#tim #navbar span.mouse:hover{cursor:pointer;font-weight:600}#tim #navbar span.mouse:hover i{color:darkred;transform:scale(1.5);transform-origin:50% 50%;line-height:1em;-webkit-font-smoothing:antialiased;width:1em;display:inline-block}#tim #listing{flex-grow:1;transition:margin-left .5s;border-right:solid 1px gray;box-shadow:0 8px 8px 0 rgba(0,0,0,.6);padding-left:20px;padding-right:20px}#tim #listing .input-icons i{position:absolute}#tim #listing .input-icons{width:100%;margin:20px}#tim #listing .icon-search{padding:15px;font-size:1.2rem}#tim #listing .input-field{padding:10px;text-align:center}#tim #listing input{padding:5px 0 5px 40px;width:100%;border:1px solid #add8e