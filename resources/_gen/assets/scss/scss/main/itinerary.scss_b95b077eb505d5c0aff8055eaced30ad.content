#itinerary .hr-text{line-height:1em;position:relative;outline:0;border:0;color:#000;text-align:left;height:1.5em}#itinerary .hr-text:before{content:'';background:linear-gradient(to right,#000,#000,#000);position:absolute;left:0;top:50%;width:100%;height:2px}#itinerary .hr-text:after{content:attr(data-content);position:relative;display:inline-block;color:#000;padding:0 .5em;line-height:1.5em;color:#000;background-color:#90ee90}#itinerary h2{margin-bottom:0}#itinerary h3{text-align:center}#itinerary .container{max-width:95%}#itinerary .pagina{display:flex;flex:1 0 auto}#itinerary .itinerary{background-color:#90ee90;padding:25px;width:33%}#itinerary .itinerary h5{margin-bottom:0}#itinerary .workspace{background-color:beige;padding:10px;width:33%}#itinerary .search{background-color:#d3d3d3;padding:25px;width:33%}#itinerary .icon-bed{color:#000}#itinerary .box{border:3px solid #666;background-color:#ddd;border-radius:.5em;padding:10px;cursor:move}#itinerary .box.over{border:3px dotted #666}#itinerary .droptarget{min-height:100px}#itinerary select{padding:5px 10px;width:100%}#itinerary .btn{background-color:blue}#itinerary .list-group-item{position:relative;display:block;padding:.75rem 1.25rem;background-color:#fff;border:1px solid rgba(0,0,0,.125)}#itinerary .list-group-item+.list-group-item{border-top-width:0}#itinerary .glyphicon-move{cursor:move;cursor:-webkit-grabbing}